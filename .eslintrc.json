{
	"extends": ["next/core-web-vitals", "eslint:recommended", "plugin:react/recommended"],

	"env": {
		"browser": true,
		"jest": true,
		"es6": true
	},
	"plugins": ["import"],
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"rules": {
		"no-console": "warn",
		"no-eval": "error",
		"import/first": "error",
		"react/react-in-jsx-scope": "off"
	}
}
